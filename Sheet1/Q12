import java.util.*;
public class Q12 {

	public static void main(String[] args) {
		Scanner input = new Scanner(System.in);
		int num = input.nextInt();
		if(num == 1)
			System.out.print(0);
		if(num == 2)
			System.out.print(1);
		long a = 0 , b = 1 , fib = 0;
		for(int i = 3 ; i <= num ; i++) {
			fib = a + b;
			a = b;
			b = fib;
		}
		System.out.print(fib);
	}

}
